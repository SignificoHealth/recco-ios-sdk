  "components": {
    "schemas": {
      "ContentId": {
        "required": [
          "catalogId",
          "itemId"
        ],
        "type": "object",
        "properties": {
          "itemId": {
            "type": "string"
          },
          "catalogId": {
            "type": "string"
          }
        }
      },
      "ContentType": {
        "type": "string",
        "enum": [
          "articles"
        ]
      },
      "Status": {
        "type": "string",
        "enum": [
          "no_interaction",
          "viewed"
        ]
      },
      "UpdateStatus": {
        "required": [
          "contentId",
          "contentType",
          "status"
        ],
        "type": "object",
        "properties": {
          "contentId": {
            "$ref": "#/components/schemas/ContentId"
          },
          "contentType": {
            "$ref": "#/components/schemas/ContentType"
          },
          "status": {
            "$ref": "#/components/schemas/Status"
          }
        }
      },
      "ApiError": {
        "required": [
          "errorCode",
          "message",
          "traceId"
        ],
        "type": "object",
        "properties": {
          "errorCode": {
            "type": "string",
            "enum": [
              "UNKNOWN",
              "RESOURCE_NOT_FOUND",
              "UNAUTHORIZED",
              "APP_DISABLED",
              "BAD_REQUEST",
              "CONFLICT"
            ]
          },
          "message": {
            "type": "string"
          },
          "traceId": {
            "type": "string"
          }
        }
      },
      "Rating": {
        "type": "string",
        "enum": [
          "like",
          "dislike",
          "not_rated"
        ]
      },
      "UpdateRating": {
        "required": [
          "contentId",
          "contentType",
          "rating"
        ],
        "type": "object",
        "properties": {
          "contentId": {
            "$ref": "#/components/schemas/ContentId"
          },
          "contentType": {
            "$ref": "#/components/schemas/ContentType"
          },
          "rating": {
            "$ref": "#/components/schemas/Rating"
          }
        }
      },
      "UpdateBookmark": {
        "required": [
          "bookmarked",
          "contentId",
          "contentType"
        ],
        "type": "object",
        "properties": {
          "contentId": {
            "$ref": "#/components/schemas/ContentId"
          },
          "contentType": {
            "$ref": "#/components/schemas/ContentType"
          },
          "bookmarked": {
            "type": "boolean"
          }
        }
      },
      "CreateQuestionnaireAnswer": {
        "required": [
          "questionId",
          "questionnaireId",
          "type"
        ],
        "type": "object",
        "properties": {
          "multichoice": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            }
          },
          "numeric": {
            "type": "number",
            "format": "double"
          },
          "questionnaireId": {
            "type": "string"
          },
          "questionId": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/QuestionAnswerType"
          }
        }
      },
      "QuestionAnswerType": {
        "type": "string",
        "enum": [
          "multichoice",
          "numeric"
        ]
      },
      "CreateMetricSession": {
        "required": [
          "seconds"
        ],
        "type": "object",
        "properties": {
          "seconds": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "PATReferenceDelete": {
        "required": [
          "tokenId"
        ],
        "type": "object",
        "properties": {
          "tokenId": {
            "type": "string",
            "description": "PAT's id"
          }
        }
      },
      "PAT": {
        "required": [
          "accessToken",
          "creationDate",
          "expirationDate",
          "tokenId"
        ],
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "expirationDate": {
            "type": "string",
            "format": "date-time"
          },
          "tokenId": {
            "type": "string"
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "AppUser": {
        "required": [
          "id",
          "isOnboardingQuestionnaireCompleted"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "isOnboardingQuestionnaireCompleted": {
            "type": "boolean"
          }
        }
      },
      "Topic": {
